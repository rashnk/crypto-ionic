    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0-0",
    "@vue/test-utils": "^2.0.0-0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0",
    "node-sass": "^6.0.0",
    "sass": "^1.34.0",
    "sass-loader": "^10.1.1",
    "vue-jest": "^5.0.0-0"
  },
  "description": "An Ionic project"
